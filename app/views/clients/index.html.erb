<p id="notice"><%= notice %></p>
<div class="row">
  <div class="small-12 columns">
    <h1>Listing Clients</h1>
  </div>
</div>
<div class="row">
  <div class="small-12 columns">
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Phone</th>
          <th>Message</th>
          <th>Origin</th>
          <th>Date</th>
        </tr>
      </thead>
      <tbody>
        <% @clients.each do |client| %>
        <tr>
          <td><%= client.name %></td>
          <td><%= client.email %></td>
          <td><%= client.phone %></td>
          <td><%= client.message %></td>
          <td><%= client.origin %></td>
          <td><%= client.created_at %></td>
          <td><%= link_to 'Destroy', client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
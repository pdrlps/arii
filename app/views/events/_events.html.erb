<% events.each do |event| %>
<div class="row agent-info info-enabled">
  <div class="small-12 medium-1 large-1 columns left">
    <%= link_to event.id, event %>
  </div>
  <div class="small-12 medium-6 large-6 columns">
    <% event.payload.each do |k,v| %>
    <div class="row">
      <div class="small-12 medium-3 large-3 columns">
        <h6><strong><%= k %></strong></h6>
      </div>
      <div class="small-12 medium-9 large-9 columns">
        <%= v %>
      </div>
    </div>
    <% end %>
  </div>
  <div class="small-12 medium-3 large-3 columns">
    <%= event.created_at %>
  </div>
  <div class="small-12 medium-2 large-2 columns right">
    <%= link_to 'View', event, :class => 'button small info radius icon-view right' %>
  </div>
</div>
<% end %>
<div class="small-12 medium-12 large-12 columns">
  <div class="row">
    <div class="small-12 pagination-centered">
      <%= paginate @events, :remote => true, :param_name => 'page' %>
    </div>
  </div>
</div>
<% events.each do |event| %>
<div class="row agent-info info-secondary">
  <div class="small-12 medium-1 large-1 columns left">
    <%= link_to event.id, event %>
  </div>
  <div class="small-12 medium-10 large-10 columns">
    <% event.payload.each do |k,v| %>
    <div class="row">
      <div class="small-12 medium-3 large-3 columns">
        <h6><strong><%= k %></strong></h6>
      </div>
      <div class="small-12 medium-9 large-9 columns">
        <%= v %>
      </div>
    </div>
    <% end %>
  </div>
  <div class="small-12 medium-1 large-1 columns right">
    <%= link_to '', event, :class => 'button small primary radius icon-view right' %>
  </div>
</div>
<% end %>